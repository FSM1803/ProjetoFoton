<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cardview_dark_background"
    tools:context=".criacao_contato">

    <TextView
        android:id="@+id/logradouroTextView"
        android:layout_width="277dp"
        android:layout_height="31dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="620dp"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cidadeTextView"
        android:layout_width="147dp"
        android:layout_height="27dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="664dp"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/bairroTextView"
        android:layout_width="276dp"
        android:layout_height="27dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="584dp"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/EstadoTextView"
        android:layout_width="273dp"
        android:layout_height="27dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="544dp"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="41dp"
        android:layout_height="44dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="356dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_call_24" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="41dp"
        android:layout_height="44dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="420dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/calendario" />


    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="41dp"
        android:layout_height="44dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="236dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/perfil" />

    <ImageView
        android:id="@+id/camera"
        android:layout_width="139dp"
        android:layout_height="126dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="137dp"
        android:background="@drawable/imagemview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/perfil" />

    <EditText
        android:id="@+id/datanascimento"
        android:layout_width="314dp"
        android:layout_height="43dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/borderradius_edittext"
        android:ems="10"
        android:hint="Data de nascimento"
        android:inputType="phone"
        android:padding="8dp"
        android:textColorHint="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/sobrenome"
        android:layout_width="314dp"
        android:layout_height="43dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="39dp"
        android:background="@drawable/borderradius_edittext"
        android:ems="10"
        android:hint="Sobrenome"
        android:inputType="text"
        android:padding="8dp"
        android:textColorHint="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/estado"
        android:layout_width="71dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="544dp"
        android:text="Estado:"
        android:textColor="#FFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/bairro"
        android:layout_width="73dp"
        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="588dp"
        android:text="Bairro:"
        android:textColor="#FFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cidade"
        android:layout_width="73dp"
        android:layout_height="24dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="664dp"
        android:text="Cidade:"
        android:textColor="#FFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/rua"
        android:layout_width="73dp"
        android:layout_height="24dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="624dp"
        android:text="Rua:"
        android:textColor="#FFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/criar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="@color/cardview_dark_background"
        android:text="Criar contato"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buscar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="677dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="@color/cardview_dark_background"
        android:text="buscar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/cep"
        android:layout_width="314dp"
        android:layout_height="43dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="480dp"
        android:layout_marginEnd="38dp"
        android:background="@drawable/borderradius_edittext"
        android:ems="10"
        android:hint="CEP"
        android:inputType="number"
        android:maxLength="9"
        android:padding="8dp"
        android:textColorHint="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="314dp"
        android:layout_height="43dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/borderradius_edittext"
        android:ems="10"
        android:hint="Telefone"
        android:inputType="phone"
        android:padding="8dp"
        android:textColorHint="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/nome"
        android:layout_width="314dp"
        android:layout_height="43dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="236dp"
        android:layout_marginEnd="38dp"
        android:background="@drawable/borderradius_edittext"
        android:ems="10"
        android:hint="Nome"
        android:inputType="text"
        android:padding="8dp"
        android:textColorHint="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="58dp"
        android:layout_height="30dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/buttonimage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_arrow_back_24" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="44dp"
        android:layout_height="43dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="481dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_add_location_24" />


</androidx.constraintlayout.widget.ConstraintLayout>